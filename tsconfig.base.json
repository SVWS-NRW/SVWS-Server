{
	"compilerOptions": {
		"composite": false,
		"isolatedModules": true,
		"skipLibCheck": true,
		"useDefineForClassFields": true,
		"target": "esnext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"importHelpers": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"jsx": "preserve",
		"declaration": true,
		"declarationMap": true,
		"lib": ["esnext", "dom"],
		"allowJs": true,
		"checkJs": true,
		"newLine": "lf",
		"rootDir": ".",
		"types": ["vite/client"]
	},
}
