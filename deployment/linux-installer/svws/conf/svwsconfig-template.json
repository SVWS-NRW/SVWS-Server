{
  "EnableClientProtection" : null,
  "DisableDBRootAccess" : false,
  "DisableAutoUpdates" : false,
  "UseHTTPDefaultv11" : false,
  "PortHTTPS" : 8443,
  "UseCORSHeader" : false,
  "TempPath" : "tmp",
  "TLSKeyAlias" : "${SVWS_TLS_KEY_ALIAS}",
  "TLSKeystorePath" : "${SVWS_TLS_KEYSTORE_PATH}",
  "TLSKeystorePassword" : "${SVWS_TLS_KEYSTORE_PASSWORD}",
  "ClientPath" : "./client",
  "LoggingEnabled" : true,
  "LoggingPath" : "logs",
  "DBKonfiguration" : {
    "dbms" : "MARIA_DB",
    "location" : "${MYSQL_HOST}",
    "defaultschema" : "${MYSQL_DATABASE}",
    "SchemaKonfiguration" : [ {
      "name" : "${MYSQL_DATABASE}",
      "svwslogin" : false,
      "username" : "${MYSQL_USER}",
      "password" : "${MYSQL_PASSWORD}"
    } ]
  }
}